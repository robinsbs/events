<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SkyBlueSoftware.Events.App">
    
    <DataTemplate DataType="{x:Type vm:Body}">
        <DockPanel>
            <ItemsControl DockPanel.Dock="Left" ItemsSource="{Binding Publishers}" />
            <ItemsControl ItemsSource="{Binding Subscribers}" />
        </DockPanel>
    </DataTemplate>

</ResourceDictionary>
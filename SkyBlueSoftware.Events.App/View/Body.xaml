<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SkyBlueSoftware.Events.App">
    
    <DataTemplate DataType="{x:Type vm:Body}">
        <StackPanel>
            <Button Content="Event1" Command="{Binding Event1Command}" />
            <Button Content="Event2" Command="{Binding Event2Command}" />
            <ItemsControl ItemsSource="{Binding Subscribers}" />
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>